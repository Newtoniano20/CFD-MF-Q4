FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
   
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 300;
    }

   front    
    {
        type            empty;	
    }

   back    
    {
        type            empty;	
    }

    top    
    {
        type            zeroGradient;	
    }
     

    bottom    
    {
        type            zeroGradient;	
    }

    wall
    {
        type           zeroGradient;
    }



}

